@inproceedings{victorien_barbet_stability_2017,
  author = {{Victorien Barbet} and {Noe Guiraud} and {Juliette Rouchier}},
  title = {Stability and representativeness of {NPO} networks : the influence
	of communication on norms},
  booktitle = {Procedings of the 13th {Annual} {Conference} of the {European} {Social} {Simulation} {Association}},
  year = {2017},
  address = {Dublin},
  month = {sep},
  abstract = {When it comes to organize collective actions except for national or
	local public power, NPO seems to be one of the privileged form chosen
	by people. It has the advantage to offer a proper juridical form
	to the organisation, facilitating actions in the public area. In
	our paper we consider NPO in their french conception, called ”associations”
	and ruled by the 1901 law. They are democratic structures, where
	a board is elected, general assembly has to be scheduled every year
	with the obligation to invite all the members of the NPO to participate.
	During this assembly the board is elected, the new budget is voted
	and the realised one has to be explained. Most of the time at this
	stage NPO are locally created and gather few people, limiting their
	visibility and means available for large range of actions. To compensate
	these weaknesses NPO which fulfil same or similar goals can gather
	in networks. The network is itself a NPO with a board, a budget and
	a general assembly, it is equivalent to a NPO of NPO. Most of the
	time, to achieve a greater visibility the network has to harmonize
	the practices among its members by imposing norms (”the good practices”).
	Unfortunately this harmonization leads to numerous conflicts which
	can end up with split in network and/or large leaving among its members.
	Indeed even if all members share common objectives for the network,
	they all come with their own point of view on the practices to implement.
	Conflicts arise from the confrontation of these different points
	of view and the strong willingness of members to apply norms close
	enough to their own practices.},
  keywords = {SSC 2017, Diffusion, Victorien},
}

@misc{victorien_barbet_haggling_2017,
  author = {{Victorien Barbet} and {Noe Guiraud} and {Vincent Lapperiere} and
	{Juliette Rouchier}},
  title = {Haggling on {Values}: {Towards} {Consensus} or {Trouble}},
  month = {jun},
  year = {2017},
  abstract = {Based on a field study we have led, we propose an ABM replicating
	the evolution of action oriented groups (like NPO) due to disagreement
	among members on the practices to implement. Looking at the stability
	and representativeness (ability of groups to federate) we introduce
	the possibility for group to communicate around their practices to
	their members. We have tested for three levels (to whom it is addressed)
	and four types (how it influences agents) of communication. We have
	shown that communication on norms despite it is not a panacea has
	a positive impact on both stability and representativeness at the
	condition of being well designed: broad and non aggressive. Interestingly
	its effects increases with the complexity of discussions in groups.
	Communication around norms is then a valuable tool to implement in
	groups wanting to improve their democratic practices without having
	a large drawback on their stability},
  keywords = {Victorien},
  urldate = {2017-06-30},
}

