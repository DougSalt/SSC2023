@article{lebow_fear_2006,
  author = {Lebow, Richard Ned},
  title = {Fear, interest and honour: outlines of a theory of {International} {Relations}},
  journal = {International Affairs},
  year = {2006},
  volume = {82},
  pages = {431--448},
  number = {3},
  month = {may},
  abstract = {This article justifies the need for a new paradigm of politics based
	on the spirit. Fifth-century Greeks considered it one of three psychic
	drives, the other two being appetite and reason. The spirit reflects
	the universal human need for self-esteem, which is achieved by emulating
	the skills, character and accomplishments of people considered praiseworthy
	by our society. According to Socrates, the spirit loves honour and
	victory. It responds with anger to any impediment to self-assertion
	in private or civic life. It desires to avenge all slights of honour
	or standing to ourselves and our friends. It demands immediate action,
	which can result in ill-considered behaviour, but can be advantageous
	in circumstances where rapid responses are necessary. Even a cursory
	examination of international relations in this modern period indicates
	the continuing importance of the spirit, and the need to conceptualize
